{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/server/api/upload.ts"
    },
    {
      "src": "/api/marketplace",
      "dest": "/server/api/marketplace.ts"
    }
  ]
}